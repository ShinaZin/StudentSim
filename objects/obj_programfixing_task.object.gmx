var symb_color = $FFFF00;//bgr
var numb_color = $5050FF;
draw_set_halign(fa_left);
draw_set_font(font_Rus);

var x_interval = 12;
var y_interval = 20;
var col=0, row=0;
var char = "";
var prevchar="";
for(i=1;i&lt;=string_length(text);i++){
  prevchar = char;
  char = string_char_at(text,i);
  if (char=="\") continue;
  if (char=="#" &amp;&amp; prevchar!="\") {
     row++; 
     col=0;
  }
  if (char=="#" &amp;&amp; prevchar=="\") {char="\#";col++}
  // Coloring
  if (string_pos(char,"()=/\!#%^&amp;*-+;&lt;&gt;{}.,")&gt;0){
     draw_set_colour(symb_color);
  }else if (string_pos(char,"0123456789")&gt;0){
     draw_set_colour(numb_color);
  }else{
     draw_set_colour(text_color);
  }
  draw_text(x+col*x_interval,y+row*y_interval,char);
  col++;
}
draw_text(x,y-30,"Задание номер "+task_id+":");
