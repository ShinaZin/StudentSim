    if !keyboard_check(vk_shift) {
     sy = py
     sx = px
    }
    view_caret()
    exit
   }
   px += string_pos(" ",str2)
   if !keyboard_check(vk_shift) {
    sy = py
    sx = px
   }
   view_caret()
   exit
  }
  
